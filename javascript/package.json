    "build:watch": "esbuild --bundle --format=esm --outfile=lib/index.js --sourcemap --watch src/index.ts"
  },
  "devDependencies": {
    "@types/react": "^18.2.6",
    "esbuild": "^0.17.19",
    "typescript": "^5.0.4"
  }
}

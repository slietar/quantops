{
  "name": "quantops",
  "version": "0.0.0",
  "type": "module",
  "main": "lib/index.js",
  "types": "types/index.d.ts",
  "scripts": {
    "build": "esbuild --bundle --format=esm --minify --outfile=lib/index.js src/index.ts",
